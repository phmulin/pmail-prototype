{
  "manifest_version": 2,
  
  "name": "Pmailbox",
  "description": "Hello World. Version 1a",
  "version": "1",

  "permissions": [
    "tabs","<all_urls>"
    ],

  "browser_action": {
    "default_icon": "img/icon.png"
  },

  "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "css": ["css/pmail.css"],
        "js": ["js/jquery.js","js/firebase.js","js/updateUI.js","js/main.js","js/ai.js","js/pmail.js"],
        "run_at": "document_end"
        }
    ] 
}